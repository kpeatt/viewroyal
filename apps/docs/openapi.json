                            "type": "boolean"
                          },
                          "has_transcript": {
                            "type": "boolean"
                          },
                          "organization": {
                            "type": [
                              "string",
                              "null"
                            ]
                          },
                          "summary": {
                            "type": [
                              "string",
                              "null"
                            ]
                          }
                        },
